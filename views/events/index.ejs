
<div class="container mt-4">

<h3>Upcoming Events</h3>

<div class="row">

<% events.forEach(e=>{ %>

<div class="col-md-4">

<div class="card mb-4">

<img src="<%= e.image %>" class="card-img-top">

<div class="card-body">
<h5><%= e.title %></h5>
<p><%= e.description %></p>
<a href="/events/<%= e._id %>" class="btn btn-primary">View</a>
</div>

</div>
</div>

<% }) %>

</div>
</div>
